<tr>
  <td><%= pb_rails("link", props: { text: routine.name, href: routine_path(routine) }) %></td>
  <td><%= pb_rails("button", props: { text: "Edit", aria: { label: "Edit Routine" }, tag: "a", link: edit_routine_path(routine), variant: 'secondary' }) %></td>
  <td>
    <%= pb_rails("button", props: { text: 'Delete', link: routine_path(routine), aria: { label: "Delete Routine" }, variant: 'danger', html_options: { 'data-method': 'delete' } }) %>
  </td>
  <td><%= pb_rails("button", props: { text: "Workout", aria: { label: "Begin Workout" }, tag: "a", link: workout_routine_path(routine), variant: 'secondary' }) %></td>
</tr>
